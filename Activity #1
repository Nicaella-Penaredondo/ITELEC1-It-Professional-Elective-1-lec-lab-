public class BasicMath {
    public static void main(String[] args) {

        int num = 99;

        if (num == 100) {
            System.out.println("100");
        }
        else if (num == 99) {
            System.out.println("yes");
        }
        
        else {
           System.out.println("no");
        }
    }
}

      
